{
    "name": "webpack2-react-hbq",
    "version": "1.0.0",
    "description": "react antd webpack2",
    "author": "hubiqing01@58ganji.com",
    "keywords": [
        "ant",
        "react",
        "admin",
        "frontend"
    ],
    "scripts": {
        "start": "better-npm-run start",
        "clean": "better-npm-run remove",
        "open:dist": "lite-server --config \"./dev-config/lite-server-config.js\" ",
        "dist:dev": "better-npm-run dist:dev",
        "dist:prod": "better-npm-run dist:prod",
        "tree:dir": "treee -l 2 --ignore node_modules/,.git/"
    },
    "betterScripts": {
        "start": {
            "command": "better-npm-run remove && better-npm-run dev"
        },
        "remove": {
            "command": "rimraf dist"
        },
        "dev": {
            "command": "webpack-dev-server --config \"./dev-config/webpack.config.js\" --progress --colors  --hot ",
            "env": {
                "NODE_ENV": "development",
                "IS_PROXY": true
            }
        },
        "dist:dev": {
            "command": "better-npm-run remove && webpack --config \"./dev-config/webpack.config.js\" --progress --colors --display-error-details",
            "env": {
                "NODE_ENV": "development"
            }
        },
        "dist:prod": {
            "command": "better-npm-run remove && webpack --config \"./dev-config/webpack.config.js\" --progress --colors --display-error-details",
            "env": {
                "NODE_ENV": "production"
            }
        }
    },
    "dependencies": {
        "classnames": "^2.2.5",
        "isomorphic-fetch": "^2.2.1",
        "lodash": "^4.17.4",
        "moment": "^2.18.1",
        "react": "15.3.0",
        "react-addons-update": "^15.4.2",
        "react-cookie": "^1.0.4",
        "react-dom": "15.3.0",
        "react-router": "3.0.0"
    },
    "devDependencies": {
        "autoprefixer": "^6.7.6",
        "babel-core": "^6.21.0",
        "babel-eslint": "6.0.0",
        "babel-loader": "^6.2.10",
        "babel-plugin-import": "1.1.0",
        "babel-polyfill": "^6.20.0",
        "babel-preset-latest": "^6.16.0",
        "babel-preset-react": "^6.16.0",
        "babel-preset-stage-0": "^6.16.0",
        "better-npm-run": "0.0.14",
        "compression-webpack-plugin": "^0.3.2",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "0.23.1",
        "eslint": "2.7.0",
        "eslint-config-airbnb": "6.x",
        "eslint-plugin-react": "4.x",
        "extract-text-webpack-plugin": "^2.1.0",
        "file-loader": "0.9.0",
        "html-webpack-plugin": "2.28.0",
        "less": "2.7.1",
        "less-loader": "2.2.3",
        "lite-server": "^2.2.2",
        "postcss-flexibility": "^1.1.0",
        "postcss-loader": "^1.3.3",
        "postcss-px2rem": "^0.3.0",
        "react-hot-loader": "^1.3.1",
        "require-dir": "^0.3.1",
        "rimraf": "^2.6.1",
        "source-map-loader": "^0.2.0",
        "strip-loader": "^0.1.2",
        "style-loader": "0.13.1",
        "tree-cli": "^0.4.12",
        "url-loader": "0.5.7",
        "webpack": "^2.2.1",
        "webpack-dev-server": "^2.4.1",
        "webpack-md5-hash": "^0.0.5"
    },
    "repository": "https://github.com/githbq/hbq-simple-webpack2-react.git",
    "license": "MIT"
}
